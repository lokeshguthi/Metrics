<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
    <rule>
        <from>^/docs(/(:?.*/)?[^./]+)?$</from>
        <to type="redirect">/exclaim/docs$1/</to>
    </rule>
    <rule>
        <from>^/docs/(.*/)?$</from>
        <to>/docs/$1index.html</to>
    </rule>
</urlrewrite>
